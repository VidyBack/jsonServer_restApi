{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "headers": {
          "Vercel-CDN-Cache-Control": "max-age=3600",
          "CDN-Cache-Control": "max-age=60",
          "Cache-Control": "max-age=10",
          "x-vidyback": "ABC:123"
        }
      }
    ]
  }