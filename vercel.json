{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "headers": {
          "cache-control": "s-maxage=1, stale-while-revalidate=59",
          "x-vidyback": "ABC:123"
        }
      }
    ]
  }